{"version":3,"file":"static/js/654.1ce3ffa7.chunk.js","mappings":"qLAEMA,EAAAA,SAAAA,IAAAA,EAAAA,EAAAA,GAAAA,EAAAA,GAAAA,IAAAA,GAAAA,EAAAA,EAAAA,GAAAA,GAAAA,SAAAA,IAAAA,IAAAA,GAAAA,EAAAA,EAAAA,GAAAA,KAAAA,GAAAA,IAAAA,IAAAA,EAAAA,UAAAA,OAAAA,EAAAA,IAAAA,MAAAA,GAAAA,EAAAA,EAAAA,EAAAA,EAAAA,IAAAA,EAAAA,GAAAA,UAAAA,GA0BD,OA1BCA,EAAAA,EAAAA,KAAAA,MAAAA,EAAAA,CAAAA,MAAAA,OAAAA,KACFC,MAAM,CACFC,KAAK,GADH,EASNC,QAAU,WACNC,OAAOC,QAAQC,IAAI,EACtB,IAEDC,MAAO,WACyB,KAAzB,EAAKC,WAAW,OACf,EAAKL,UAELC,OAAOK,SAASC,KAAO,EAAKF,WAAW,MAE9C,IAEDG,WAAW,WACP,EAAKV,MAAMC,KAAK,EAChB,EAAKU,SAAS,EAAKX,OACnBY,aAAa,EAAKC,MACrB,IA6BA,OA7BA,kCArBD,SAAWC,EAAMC,GACb,OAAIC,KAAKC,MAAMC,OAAOC,IAAIL,GAAcE,KAAKC,MAAMC,OAAOC,IAAIL,GACvDC,CACV,G,+BAoBD,WAAqB,IAAD,OAEhBK,SAASC,cAAc,QAAQC,iBAAiB,QAAQN,KAAKN,YAE7DM,KAAKH,MAAQU,aAAY,WACjB,EAAKvB,MAAMC,MAAM,EACjB,EAAKU,SAAS,EAAKX,OAEC,GAAjB,EAAKA,MAAMC,MAAQ,EAAKC,SAC9B,GAAC,IACT,G,kCAED,WACIc,KAAKH,OAASD,aAAaI,KAAKH,MACnC,uBACD,WACI,OAAO,+BACH,SAAC,KAAD,CACIW,OAAO,QACPC,MAAOT,KAAKT,WAAW,QAAQ,gBAC/BmB,SAAUV,KAAKT,WAAW,MAAM,6BAChCoB,MAAO,EACH,UAAC,IAAD,CAAQC,KAAK,UAAwBC,QAASb,KAAKd,QAAnD,yBAA+Dc,KAAKhB,MAAMC,KAAK,EAAE,IAAIe,KAAKhB,MAAMC,KAAK,IAAI,KAA9E,YAC3B,SAAC,IAAD,CAAQ2B,KAAK,GAAeC,QAASb,KAAKV,MAA1C,yBAAoB,aAInC,OAvDCP,CAAc+B,EAAAA,WAyDpB,W","sources":["pages/admin/handle/fail.jsx"],"sourcesContent":["import React from \"react\";\r\nimport { Button, Result } from 'antd';\r\nclass Index extends React.Component {\r\n    state={\r\n        time:5,//倒计时\r\n    }\r\n    //获取参数 没有时获取默认值\r\n    get_params(name, val) {\r\n        if (this.props.params.get(name)) return this.props.params.get(name);\r\n        return val;\r\n    }\r\n    //返回\r\n    go_back = () =>{\r\n        window.history.go(-1);//返回到操作页面\r\n    }\r\n    //重试\r\n    retry= () =>{\r\n        if(this.get_params('url')===''){\r\n            this.go_back()\r\n        }else{\r\n            window.location.href = this.get_params('url');\r\n        }\r\n    }\r\n    //body原始被点击事件\r\n    body_click=()=>{\r\n        this.state.time=0;\r\n        this.setState(this.state);\r\n        clearTimeout(this.timer);//卸载定时器\r\n    }\r\n    //组件加载\r\n    componentDidMount() {\r\n        //如果用户除非了点击事件 则取消自动跳转\r\n        document.querySelector(\"body\").addEventListener('click',this.body_click);\r\n\r\n        this.timer = setInterval(() => {\r\n                this.state.time-=1;\r\n                this.setState(this.state)\r\n                //计时已满 自动跳转\r\n                if(this.state.time==0)this.go_back()\r\n            },1000);\r\n    }\r\n    //组件卸载\r\n    componentWillUnmount() {\r\n        this.timer && clearTimeout(this.timer);\r\n    }\r\n    render() {\r\n        return <>\r\n            <Result\r\n                status=\"error\"\r\n                title={this.get_params('title','失败')}\r\n                subTitle={this.get_params('msg','操作失败!')}\r\n                extra={[\r\n                    <Button type=\"primary\" key=\"go_back\" onClick={this.go_back}>返回{this.state.time>0?\"(\"+this.state.time+\")\":\"\"}</Button>,\r\n                    <Button type=\"\" key=\"retry\" onClick={this.retry}>重试</Button>,\r\n                ]}\r\n            />\r\n        </>\r\n    }\r\n}\r\nexport default Index\r\n"],"names":["Index","state","time","go_back","window","history","go","retry","get_params","location","href","body_click","setState","clearTimeout","timer","name","val","this","props","params","get","document","querySelector","addEventListener","setInterval","status","title","subTitle","extra","type","onClick","React"],"sourceRoot":""}