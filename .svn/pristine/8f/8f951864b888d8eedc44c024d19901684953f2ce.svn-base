lt=\"product\"/>\r\n                    </a>\r\n                    <div className=\"product_badge\">\r\n                        <div className=\"product_label new\"><span>new</span></div>\r\n                        <div className=\"product_label discount\"><span>0%</span></div>\r\n                    </div>\r\n                </figure>\r\n                <div className=\"product_caption\">\r\n                    <p className=\"product_name\">\r\n                        {/*<!--产品名称-->*/}\r\n                        <a href=\"/shop/product/show/id/1\">iphone13</a>\r\n                    </p>\r\n                    <div className=\"price_box\">\r\n                        <span className=\"price_regular\">¥5399.00</span>\r\n                        <span className=\"price_old\"><del>¥6399.00</del></span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n      </>\r\n    )\r\n  }\r\n}\r\nexport default ProductDisplay","/*\r\n    用户首页\r\n    作者:chenpinzhong\r\n    开发备注:主要实现网站首页\r\n*/\r\nimport './css/index.css'\r\nimport Slide from '../../components/common/Slide' //幻灯片组件\r\nimport ProductDisplay from '../../components/common/ProductDisplay' //产品显示组件\r\n//幻灯片\r\nfunction slide_list(){\r\n    return (<>\r\n        <div className='element'>\r\n            <img src={process.env.PUBLIC_URL + '/images/s3.jpg'} alt=\"\"/>\r\n        </div>\r\n        <div className='element'>\r\n            <img src={process.env.PUBLIC_URL + '/images/s2.jpg'} alt=\"\"/>\r\n        </div>\r\n    </>)\r\n}\r\n//用户首页\r\nfunction Index(){\r\n    return (\r\n        <>\r\n            <main>\r\n                {/*轮播图*/}\r\n                <section className='slider_area'>\r\n                    <div className='slide_frame'>\r\n                        <Slide width='1200' height='500' slide_list={slide_list} />\r\n                    </div>\r\n                </section>\r\n                <section className='section_space'>\r\n\t\t\t        <div className='container'>\r\n                        <div className='slide_frame'>\r\n                            <div className=\"section_title\">\r\n                                <h2>猜你喜欢</h2>\r\n                                <p>大家喜欢的好物</p>\r\n                            </div>\r\n                            {/*产品展示模块*/}\r\n                            <div className='row mtn-40'>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                            </div>\r\n                            <div className='row mtn-40'>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                            </div>\r\n                            <div className='row mtn-40'>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                                <ProductDisplay/>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </section>\r\n            </main>\r\n        </>\r\n    )\r\n}\r\nexport default Index"],"names":["$","jQuery","fn","extend","slide","width","height","$_this","this","dom_slide","dom_element","find","element_index","element_length","length","element_width","eq","element_height","init","$this","css","on","last","next","finish","current_element","next_element","animate","left","stop","Slide","className","style","backgroundColor","props","slide_list","React","ProductDisplay","href","src","process","alt"],"sourceRoot":""}